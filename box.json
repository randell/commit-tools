{
  "alias": "box.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Herrera\\Box\\Compactor\\Php"
  ],
  "directories": [
    "src/"
  ],
  "extract": true,
  "files": ["setup.php"],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "mikey179",
        "Net",
        "phpunit",
        "phpunit-test-case",
        "Tester",
        "Tests",
        "tests",
        "yaml"
      ],
      "in": "vendor"
    }
  ],
  "git-commit": "git-commit",
  "git-version": "git-version",
  "main": "setup.php",
  "output": "box-@git-version@.phar",
  "replacements": {
    "manifest_url": "https://box-project.github.io/box2/manifest.json"
  },
  "stub": true,
  "output": "d7setup.phar"
}